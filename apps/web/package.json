{
  "name": "@presentation-maker/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:staging": "vue-tsc -b && vite build --mode staging",
    "preview": "vite preview",
    "deploy": "pnpm run build && wrangler pages deploy dist --project-name presentation-maker",
    "test": "vitest run --environment jsdom"
  },
  "dependencies": {
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@xenova/transformers": "^2.17.2",
    "html-to-image": "^1.11.13",
    "sherpa-onnx": "^1.12.17",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.3",
    "wrangler": "^4.50.0"
  }
}
{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "./dist",
        "rootDir": ".",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "@cloudflare/workers-types",
            "node"
        ]
    },
    "include": [
        "index.ts",
        "video-worker.ts",
        "env.d.ts"
    ]
}